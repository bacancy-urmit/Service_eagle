<%= render "service_center_admins/nav" %>
<div class='booked-appointment-index_page'>
  <div class='container'>
    <table class='table'>
      <thead>
        <tr>
          <th scope='col'>bokking id</th>
          <th scope='col'>quantity</th>
          <th scope='col'>vehicle name</th>
          <th scope='col'>sparepart name</th>
          <th scope='col'>price</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td scope='row'><%= @booked_appointment.token %></td>
          <td scope='row'><%= @sparepart.quantity %></td>
          <td scope='row'><%= @booked_appointment.vehicle_name %></td>
          <td scope='row'><%= @sparepart.sparepart_name %></td>
          <td scope='row'><%= @sparepart.price %></td>
          <td><%= link_to "edit", edit_used_sparepart_path(@sparepart.id) %></td>
          <td><%= link_to "delete", used_sparepart_path(@sparepart.id), method: :delete %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>