<%= render "superadmin_nav" %>
<div class='booked-appointment-index_page'>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <%= form_with model: @company, local: true do |f| %>
        <div class="form-group">
          <%= f.label :company_name %><br />
          <%= f.text_field :name, placeholder: "eg.Bajaj" %>
        </div>
        <div class="form-group">
          <%= f.label :website %><br />
          <%= f.text_field :website, placeholder: "eg.Bajaj" %>
        </div>
        <div class="form-group">
          <%= f.label :email %><br />
          <%= f.text_field :email, placeholder: "eg.Bajaj" %>
        </div>
        <%= f.submit class: "btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </div>
</div>