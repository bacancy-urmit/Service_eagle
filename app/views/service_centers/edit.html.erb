<% if current_user.has_role? :super_admin %>
<%= render "superadmin_nav" %>
<% elsif current_user.has_role? :company_admin %>
<%= render "companyadmin_nav" %>
<% end %>
<div class='booked-appointment-index_page'>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <%= form_with model: @center, local: true do |f| %>
        <div class="form-group">
          <%= f.label :name %><br />
          <%= f.text_field :name, class: "form-control" %>
        </div>


        <div class="form-group">
          <%= f.label :area %><br />
          <%= f.text_field :area, class: "form-control" %>
        </div>


        <div class="form-group">
          <%= f.label :city %><br />
          <%= f.text_field :city, class: "form-control" %>
        </div>


        <div class="form-group">
          <%= f.label :state %><br />
          <%= f.text_field :state, class: "form-control" %>
        </div>


        <div class="form-group">
          <%= f.label :pincode %><br />
          <%= f.text_field :pincode, class: "form-control" %>
        </div>


        <div class="form-group">
          <%= f.label :email %><br />
          <%= f.text_field :email, class: "form-control" %>
        </div>

        <%= f.submit class: "btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </div>
</div>