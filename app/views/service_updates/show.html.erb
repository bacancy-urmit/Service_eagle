<%= render "service_center_admins/nav" %>
<div class="booked-appointment-index_page">
  <div class="container">
    <table class='table'>
      <thead>
        <tr>
          <th scope='col'>booking id</th>
          <th scope='col'>status</th>
          <th scope='col'>description</th>
          <th scope='col'>estimation</th>
          <th scope='col'>date</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td scope='row'><%= @appoinment.token %></td>
          <td scope='row'><%= @update.service_status %></td>
          <td scope='row'><%= @update.description %></td>
          <td scope='row'><%= @update.estimation %></td>
          <td scope='row'><%= @update.created_at %></td>
          <td scope='row'><%= link_to "edit", edit_service_update_path(@update.id) %>
        </tr>
      </tbody>
    </table>
  </div>
</div>