<%= render "service_center_admins/nav" %>
  <div class="container">
    <div class="row">
      <div class="col-md-3">

        <%= form_with model: @set_service_center_count, local: true do |f| %>
        <div class="form-group">
          <%= f.label :servicecenter %>
          <%= f.text_field :service_center_id, value: @servicecenter_name, class: "form-control", readonly: true %>
        </div>

        <div class="form-group">
          <%= f.label :servicecount %>
          <%= f.number_field :count, class: "form-control" %>
        </div>

        <div class="form-group">
          <%= f.label :date %>
          <%= f.date_field :date, value: Date.tomorrow + 1, class: "form-control", readonly: true %>
        </div>

        <div class="form-group">
          <%= f.submit class: "btn btn-primary" %>
        </div>

        <% end %>
      </div>
    </div>
  </div>
</div>