<%= render "service_center_admins/nav" %>
<div class='booked-appointment-index_page'>
  <div class='container'>
    <table class='table'>
      <thead>
        <tr>
          <th scope='col'>service center name</th>
          <th scope='col'>count</th>
          <th scope='col'>date</th>
        </tr>
      </thead>
      <tbody>
        <% @counts.each do |count| %>
        <tr>
          <td scope='row'><%= count.name %></td>
          <td scope='row'><%= count.count %></td>
          <td scope='row'><%= count.date %></td>
          <td scope='row'><%= link_to "edit", edit_service_center_capacity_path(count.id) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>