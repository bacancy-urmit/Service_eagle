<%= render "nav" %>
<div class='booked-appointment-index_page'>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <%= form_with model: @service_center_admin, url: service_center_admin_path, local: true do |f| %>

        <div class="form-group">
          <%= f.label :firstname %><br />
          <%= f.text_field :firstname %>
        </div>


        <div class="form-group">
          <%= f.label :laststname %><br />
          <%= f.text_field :lastname %>
        </div>

        <div class="form-group">
          <%= f.label :username %><br />
          <%= f.text_field :username %>
        </div>

        <div class="form-group">
          <%= f.label :email %><br />
          <%= f.text_field :email %>
        </div>

        <div class="form-group">
          <%= f.label :contact %><br />
          <%= f.text_field :contact %>
        </div>
        <div class="form-group">
          <%= f.label :password %><br />
          <%= f.password_field :password %>
        </div>

        <div class="form-group">
          <%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation %>
        </div>
        <%= f.submit "update admin" %>
      </div>
    </div>
  </div>
</div>